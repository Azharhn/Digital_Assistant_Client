{
  "manifest_version": 2,
  "name": "Universal Digital Assistant (Alpha)",
  "version": "0.1",
  "description": "Building an Extension for custom user navigation which helps in assisting users to navigate to right place.",
  "permissions": ["activeTab", "declarativeContent", "storage", "webNavigation", "cookies", "tabs", "notifications", "<all_urls>"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
	{
	  "matches": ["<all_urls>"],
//	  "matches": ["*://*.vantagecircle.com/*","*://*.nistapp.com/*","*://*.localhost/*","*://*.nistapp.local/*"],
	  "js": ["js/injectscriptmodifier.js"],
	  "all_frames": false,
	  "run_at":"document_start"
	},
	{
	  "matches": ["<all_urls>"],
//	  "matches": ["*://*.vantagecircle.com/*","*://*.nistapp.com/*","*://*.localhost/*","*://*.nistapp.local/*"],
	  //"matches": ["*://*.vantagecircle.com/*","*://*.nistapp.com/*"],
	  "js": ["js/injectsdk.js"],
	  "run_at":"document_end"
//	  "run_at": "document_idle"
	}
  ],
  "web_accessible_resources": [
	"js/*",
	"assets/*",
	"css/*"
  ],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  }
  /*
  ,
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/nist-logo-16x16.png",
      "32": "images/nist-logo-32x32.png",
      "48": "images/nist-logo-48x48.png",
      "128": "images/nist-logo-128x128.png"
    }
  }
  */
}
